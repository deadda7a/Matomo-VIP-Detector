{
    "name": "deadda7a/matomo-vip-detector",
    "description": "Special IP Range detector for Matomo",
    "type": "matomo-plugin",
    "license": "MIT",
    "version": "2.0.1",
    "authors": [
        {
            "name": "Sebastian Elisa Pfeifer",
            "email": "sebastian@sebastian-elisa-pfeifer.eu"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/matomo-org/matomo.git"
    },
    "require-dev": {
        "phpstan/phpdoc-parser": "^1.28",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^11.1",
        "squizlabs/php_codesniffer": "^3.9",
        "slevomat/coding-standard": "^8.15",
        "phan/phan": "^5.4",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "spaze/phpstan-disallowed-calls": "^3.1",
        "matomo/matomo": "*",
        "matomo/network": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Piwik\\Plugins\\VipDetector\\": "./"
        }
    }
}
