includes:
    - ~/.composer/vendor/phpstan/phpstan-deprecation-rules/rules.neon
    - ~/.composer/vendor/spaze/phpstan-disallowed-calls/extension.neon
    - ~/.composer/vendor/spaze/phpstan-disallowed-calls/disallowed-dangerous-calls.neon
parameters:
    level: 6
    paths:
        - Commands
        - Dao
        - libs
        - RangeUpdater.php
        - Tasks.php
        - VipDetector.php
        - VisitorDetails.php
        - SystemSettings.php
    excludePaths:
        analyse:
            - /tmp/matomo
        analyseAndScan:
            - tests
